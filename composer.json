{
  "name": "tildbj/typo3-tco",
  "description": "Helpful library for the table configuration array (tca)",
  "type": "library",
  "keywords": ["typo3", "tca", "table configuration array"],
  "license": "GPL-2.0+",
  "authors": [
      {
      "name": "Dennis RÃ¶mmich",
      "email": "dennis@roemmich.eu",
      "homepage": "http://www.roemmich.eu",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "dennis@roemmich.eu",
    "issues": "https://github.com/TildBJ/typo3-tco/issues",
    "source": "https://github.com/TildBJ/typo3-tco"
  },
  "require": {
    "php": "^7.0 || ^7.1 || ^7.2",
    "typo3/cms-core": "^8.7 || ^9.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^6",
    "squizlabs/php_codesniffer": "^3.0",
    "phpunit/php-code-coverage": "^5.2",
    "symfony/var-dumper": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "TildBJ\\Tco\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TildBJ\\Tco\\Test\\": "tests/"
    }
  },
  "replace": {
    "typo3-ter/tco": "self.version"
  },
  "extra": {
      "typo3/cms": {
          "cms-package-dir": "{$vendor-dir}/typo3/cms"
      }
  }
}
